
void del_xy(SqList &L,ElemType x, ElemType y)
{
    int i=0,k=0;
    while(i<L.length)
    {
        if(L.data[i]>=x&&L.data[i]<=y)
           k++;
        else
           L.data[i-k]=L.data[i];
    }
    L.length=L.length-k;

